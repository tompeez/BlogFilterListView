<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html>
<f:view xmlns:f="http://java.sun.com/jsf/core" xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
    <af:document title="Test.jsf" id="d1">
        <af:messages id="m1"/>
        <af:form id="f1">
            <af:panelGridLayout id="pgl1">
                <af:gridRow height="50px" id="gr1">
                    <af:gridCell width="100%" halign="stretch" valign="stretch" id="gc1">
                        <!-- Header -->
                    </af:gridCell>
                </af:gridRow>
                <af:gridRow height="100%" id="gr2">
                    <af:gridCell width="100%" halign="stretch" valign="stretch" id="gc2">
                        <!-- Content -->
                        <af:panelGroupLayout layout="vertical" id="pgl2">
                            <af:panelHeader text="Employees" id="ph1">
                                <af:query id="qryId1" headerText="Search" disclosed="true" value="#{bindings.ImplicitViewCriteriaQuery.queryDescriptor}"
                                          model="#{bindings.ImplicitViewCriteriaQuery.queryModel}"
                                          queryListener="#{bindings.ImplicitViewCriteriaQuery.processQuery}"
                                          queryOperationListener="#{bindings.ImplicitViewCriteriaQuery.processQueryOperation}" resultComponentId="::resId1"/>
                            </af:panelHeader>
                            <af:table value="#{bindings.EmpTestViewObj1.collectionModel}" var="row" rows="#{bindings.EmpTestViewObj1.rangeSize}"
                                      emptyText="#{bindings.EmpTestViewObj1.viewable ? 'No data to display.' : 'Access Denied.'}" rowBandingInterval="0"
                                      selectedRowKeys="#{bindings.EmpTestViewObj1.collectionModel.selectedRow}"
                                      selectionListener="#{bindings.EmpTestViewObj1.collectionModel.makeCurrent}" rowSelection="single"
                                      fetchSize="#{bindings.EmpTestViewObj1.rangeSize}" id="resId1">
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.CommissionPct.label}" id="resId1c1">
                                    <af:outputText value="#{row.CommissionPct}" shortDesc="#{bindings.EmpTestViewObj1.hints.CommissionPct.tooltip}" id="ot1">
                                        <af:convertNumber groupingUsed="false" pattern="#{bindings.EmpTestViewObj1.hints.CommissionPct.format}"/>
                                    </af:outputText>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.DepartmentId.label}" id="resId1c2">
                                    <af:selectOneChoice value="#{row.bindings.DepartmentId.inputValue}" label="#{row.bindings.DepartmentId.label}"
                                                        required="#{bindings.EmpTestViewObj1.hints.DepartmentId.mandatory}"
                                                        shortDesc="#{bindings.EmpTestViewObj1.hints.DepartmentId.tooltip}" id="soc1">
                                        <f:selectItems value="#{row.bindings.DepartmentId.items}" id="si1"/>
                                        <f:validator binding="#{row.bindings.DepartmentId.validator}"/>
                                    </af:selectOneChoice>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.Email.label}" id="resId1c3">
                                    <af:outputText value="#{row.Email}" shortDesc="#{bindings.EmpTestViewObj1.hints.Email.tooltip}" id="ot3"/>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.EmployeeId.label}" id="resId1c4">
                                    <af:outputText value="#{row.EmployeeId}" shortDesc="#{bindings.EmpTestViewObj1.hints.EmployeeId.tooltip}" id="ot4">
                                        <af:convertNumber groupingUsed="false" pattern="#{bindings.EmpTestViewObj1.hints.EmployeeId.format}"/>
                                    </af:outputText>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.FirstName.label}" id="resId1c5">
                                    <af:outputText value="#{row.FirstName}" shortDesc="#{bindings.EmpTestViewObj1.hints.FirstName.tooltip}" id="ot5"/>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.HireDate.label}" id="resId1c6">
                                    <af:outputText value="#{row.HireDate}" shortDesc="#{bindings.EmpTestViewObj1.hints.HireDate.tooltip}" id="ot6">
                                        <af:convertDateTime pattern="#{bindings.EmpTestViewObj1.hints.HireDate.format}"/>
                                    </af:outputText>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.JobId.label}" id="resId1c7">
                                    <af:outputText value="#{row.JobId}" shortDesc="#{bindings.EmpTestViewObj1.hints.JobId.tooltip}" id="ot7"/>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.LastName.label}" id="resId1c8">
                                    <af:outputText value="#{row.LastName}" shortDesc="#{bindings.EmpTestViewObj1.hints.LastName.tooltip}" id="ot8"/>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.ManagerId.label}" id="resId1c9">
                                    <af:outputText value="#{row.ManagerId}" shortDesc="#{bindings.EmpTestViewObj1.hints.ManagerId.tooltip}" id="ot9">
                                        <af:convertNumber groupingUsed="false" pattern="#{bindings.EmpTestViewObj1.hints.ManagerId.format}"/>
                                    </af:outputText>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.PhoneNumber.label}" id="resId1c10">
                                    <af:outputText value="#{row.PhoneNumber}" shortDesc="#{bindings.EmpTestViewObj1.hints.PhoneNumber.tooltip}" id="ot10"/>
                                </af:column>
                                <af:column headerText="#{bindings.EmpTestViewObj1.hints.Salary.label}" id="resId1c11">
                                    <af:outputText value="#{row.Salary}" shortDesc="#{bindings.EmpTestViewObj1.hints.Salary.tooltip}" id="ot11">
                                        <af:convertNumber groupingUsed="false" pattern="#{bindings.EmpTestViewObj1.hints.Salary.format}"/>
                                    </af:outputText>
                                </af:column>
                            </af:table>
                        </af:panelGroupLayout>
                    </af:gridCell>
                </af:gridRow>
            </af:panelGridLayout>
        </af:form>
    </af:document>
</f:view>